{
  "name": "ng-siac-front-end",
  "version": "0.0.1",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --port 4291",
    "build": "ng build",
    "build:dev": "ng build --configuration dev",
    "build:qa": "ng build --configuration qa",
    "build:prod": "ng build --configuration prod",
    "watch": "ng build --watch --configuration development",
    "release": "standard-version",
    "release:beta": "standard-version --prerelease beta && npm run build",
    "release:qa": "standard-version --prerelease rc && npm run build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format:fix": "pretty-quick --staged",
    "precommit": "run-s format:fix lint"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.0",
    "@angular/cdk": "^16.2.5",
    "@angular/common": "^16.2.0",
    "@angular/compiler": "^16.2.0",
    "@angular/core": "^16.2.12",
    "@angular/forms": "^16.2.0",
    "@angular/material": "^16.2.2",
    "@angular/material-moment-adapter": "^16.2.2",
    "@angular/platform-browser": "^16.2.0",
    "@angular/platform-browser-dynamic": "^16.2.0",
    "@angular/router": "^16.2.0",
    "@angular/service-worker": "^16.2.0",
    "bootstrap": "^5.3.3",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.6.13",
    "swiper": "^10.3.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.13.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^16.2.0",
    "@angular-eslint/builder": "^16.2.0",
    "@angular-eslint/eslint-plugin": "^16.2.0",
    "@angular-eslint/eslint-plugin-template": "^16.2.0",
    "@angular-eslint/schematics": "^16.2.0",
    "@angular-eslint/template-parser": "^16.2.0",
    "@angular/cli": "^16.2.0",
    "@angular/compiler-cli": "^16.2.0",
    "@schematics/angular": "^16.2.3",
    "@types/jasmine": "~4.3.0",
    "@types/jquery": "^3.5.29",
    "@types/node": "^18.18.0",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "autoprefixer": "^10.4.7",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-modules-newline": "^0.0.6",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-sort-class-members": "^1.18.0",
    "husky": "^8.0.3",
    "jasmine-core": "~4.6.0",
    "jquery": "^3.7.1",
    "npm-run-all": "^4.1.3",
    "postcss": "^8.4.29",
    "prettier": "^2.8.8",
    "prettier-eslint": "^15.0.1",
    "pretty-quick": "^3.1.3",
    "standard-version": "^9.5.0",
    "tailwindcss": "^3.3.3",
    "typescript": "~5.1.3"
  }
}
